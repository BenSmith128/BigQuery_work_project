CREATE TABLE `mechanic_work.4_unique_dollars_hours` AS (
  SELECT
    Hours,
    0 AS Dollars
  FROM `mechanic_work.3_data_by_month`
  WHERE Hours > 0
  GROUP BY Hours

  UNION ALL

  SELECT
    0 AS Hours,
    Dollars
  FROM `mechanic_work.3_data_by_month`
  WHERE Dollars > 0
  GROUP BY Dollars
);
